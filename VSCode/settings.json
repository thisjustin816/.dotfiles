    "diffEditor.ignoreTrimWhitespace": false,
    "git.autoStash": true,
    "git.allowForcePush": true,
    "git.fetchOnPull": true,
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x64)",
    "workbench.enableExperiments": false,
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
    "powershell.codeFormatting.useCorrectCasing": true,
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 0,
    "gitlens.blame.dateFormat": "",
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Cascadia Code PL', Consolas, 'Courier New', monospace",
    "powershell.powerShellAdditionalExePaths": [

    ],
    "terminal.integrated.automationShell.windows": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "explorer.confirmDragAndDrop": false,
    "powershell.pester.outputVerbosity": "Detailed",
    "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
    "powershell.scriptAnalysis.settingsPath": "${env:APPDATA}\\Code\\User\\PSScriptAnalyzerSettings.psd1",
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "editor.fontSize": 12,
    "debug.console.fontSize": 12,
    "markdown.preview.fontSize": 12,
    "terminal.integrated.fontSize": 12,
    "explorer.confirmDelete": false,
    "powershell.pester.useLegacyCodeLens": false,
    "team.showFarewellMessage": false,
    "liveshare.allowGuestDebugControl": true,
    "liveshare.allowGuestTaskControl": true,
    "liveshare.authenticationProvider": "Microsoft",
    "liveshare.focusBehavior": "prompt",
    "liveshare.languages.allowGuestCommandControl": true,
    "liveshare.showInStatusBar": "whileCollaborating",
    "workbench.editor.decorations.badges": true,
    "workbench.editor.wrapTabs": true
