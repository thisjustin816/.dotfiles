    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "team.showWelcomeMessage": false,
    "cSpell.userWords": [
        "CICD"
    ],
    "cSpell.language": "en,en-US",
    "cSpell.enabledLanguageIds": [
        "azure-pipelines",
        "bat",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
